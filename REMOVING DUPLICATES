# 2) REMOVING DUPLICATES
    
#  1ST WAY TO SOLVE 
 list1 = [1, 2, 3, 4, 2, 3, 5, 6, 7, 8, 8]  
 new_list = list(set([x for x in list1 if list1.count(x) > 1 ]))
 

# 2ND WAY TO SOLVE 
list1 = [1, 2, 3, 4, 2, 3, 5, 6, 7, 8, 8]
new_list = []

for i in list1:
    if i not in new_list:
        new_list.append(i)
print(new_list)
